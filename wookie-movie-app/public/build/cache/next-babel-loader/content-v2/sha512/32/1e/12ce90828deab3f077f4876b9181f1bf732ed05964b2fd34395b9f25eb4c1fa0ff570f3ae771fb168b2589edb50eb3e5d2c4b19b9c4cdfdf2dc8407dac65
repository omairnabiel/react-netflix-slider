{"ast":null,"code":"import React from \"react\";\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\nimport { faStar } from \"@fortawesome/free-solid-svg-icons\";\nexport default function Ratings({\n  filled\n}) {\n  const TOTAL_STARS = 5;\n  const ICON_SIZE = '2x';\n  const FILL_COLOR = '#ffff00';\n  const EMPTY_COLOR = '#fff';\n  const stars = [];\n\n  const renderStars = () => {\n    for (let i = 1; i <= TOTAL_STARS; i++) {\n      stars.push( /*#__PURE__*/React.createElement(FontAwesomeIcon, {\n        key: i,\n        style: i < filled / 2 ? {\n          color: FILL_COLOR\n        } : {\n          color: EMPTY_COLOR\n        },\n        icon: faStar,\n        size: ICON_SIZE\n      }));\n    }\n\n    return stars;\n  };\n\n  return /*#__PURE__*/React.createElement(\"div\", null, renderStars());\n}","map":null,"metadata":{},"sourceType":"module"}